{
  "name": "sparks-backend",
  "version": "1.0.0",
  "description": "sparks backend",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "start": "node_modules/.bin/ts-node src/index.ts",
    "postinstall": "npm run build",
    "build": "tsc",
    "test": "node_modules/.bin/ts-node node_modules/.bin/tape src/**/*.test.ts",
    "lint": "node_modules/.bin/tslint src/**/*.ts",
    "commit": "git-cz",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdebaun/sparks-backend.git"
  },
  "author": "'sdebaun' <'sdebaun74@gmail.com'>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sdebaun/sparks-backend/issues"
  },
  "homepage": "https://github.com/sdebaun/sparks-backend#readme",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js"
    }
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-tap": "^5.0.0",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^0.2.0",
    "commitizen": "^2.8.4",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "cz-customizable": "^4.0.0",
    "eslint": "^1.10.3",
    "eslint-config-cycle": "^3.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-cycle": "^1.0.2",
    "eslint-plugin-no-class": "^0.1.0",
    "nodemon": "^1.9.2",
    "tape": "^4.5.1",
    "tape-async": "^2.1.0",
    "tslint": "^3.14.0",
    "ghooks": "^1.3.2",
    "validate-commit-msg": "^2.6.1"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-module-alias": "^1.5.0",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2016": "^6.11.0",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.4.0",
    "braintree": "^1.37.1",
    "braintree-node": "^1.0.9",
    "express": "^4.13.4",
    "firebase": "^2.4.1",
    "firebase-queue": "^1.2.1",
    "inflection": "^1.10.0",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "prfun": "^2.1.3",
    "ramda": "^0.21.0",
    "rx": "^4.0.8",
    "sendgrid": "^2.0.0",
    "seneca": "^2.1.0",
    "seneca-await": "0.0.12",
    "ts-node": "^1.2.2",
    "typescript": "^2.1.0-dev.20160804",
    "typings": "^1.3.2"
  }
}
