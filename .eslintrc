{
  "extends": "eslint-config-cycle",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "no-class/no-class": 0,
    "no-unused-vars": 1,
    "key-spacing": 0,
    "max-nested-callbacks": [2, 3],
    "func-names": 0,
    "no-shadow": 0,
    "max-len": [1, 160],
    "no-loop-func": 0,
  }
}
